<%= render layout: "api_navbar" do %>
    <div id="bearer" class="section">
        <h1>Invalid Bearer token</h1>
        <h3>Response</h3>
        Status code: 401
<pre><code class="lang-json" data-controller="prism">{
    "success": false,
    "token": "bearer_token.invalid",
    "message": "Invalid bearer token"
}</code></pre>
    </div>
    <hr>

    <div id="access_token" class="section">
        <h1>Invalid Access token</h1>
        <h3>Response</h3>
        Status code: 401
<pre><code class="lang-json" data-controller="prism">{
    "success": false,
    "token": "access_token.invalid",
    "message": "Access token is invalid"
}</code></pre>
    </div>
<% end %>