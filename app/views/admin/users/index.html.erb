<%= content_for :js, "admin_users_navigation" %>

<style>
    .card {
        background: #abf7b1;
        border: 2px solid #5ced73;
        border-radius: 5px;
        padding: 10px;
        margin-bottom: 10px;
        cursor: pointer;
    }
</style>

<div class="container mt-3" style="max-width: 1000px;">
    <% @users.each do |user| %>
        <%= render partial: "user_card", locals: {user: user} %>
    <% end %>
</div>